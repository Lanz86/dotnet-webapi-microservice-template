{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=;MultipleActiveResultSets=True"
  },
  "IdentityServer": {
    "TokenValidationParameters": {
      "ValidateAudience": false,
      "ValidateIssuer": true
    }
  },
  "ServicesEndpoint": {
    "IdentityServerMicroservice": "https://localhost:5001"
  },
  "Cors": {
    "Policy": "_myAllowSpecificOrigins",
    "ValidHost": "localhost"
  },
  "Swagger": {
    "MicroserviceApiTitle": "",
    "SwaggerUiTitle": ""
  }
}
